op PACGA(Rm:REG_INDEX, Rn:REG_INDEX, Rd:REG_INDEX)
    syntax = format("pacga %s, %s, %s", get64Reg(Rd), get64Reg(Rn), get64RegoSP(Rm))
    image = format("1 0 0 1 1 0 1 0 1 1 0 %s 0 0 1 1 0 0 %s%s", Rm.image, Rn.image, Rd.image)
